        "dev:app3": "nx serve example-ionic",
        "dev:app4": "nx serve example-admin",
        "dev:server": "nx run example-server:serve",
        "server:db:up": "cd apps/example-server && npx drizzle-kit up",
        "server:db:generate": "cd apps/example-server && npx drizzle-kit generate",
        "server:db:migrate": "cd apps/example-server && npx drizzle-kit migrate",
        "update1of3": "nx migrate latest --verbose",
        "update2of3": "nx migrate --run-migrations --if-exists",
        "update3of3": "rm migrations.json",
        "update-spartan-cli": "npm i -D @spartan-ng/cli@latest --legacy-peer-deps",
        "spartan-ui": "nx g @spartan-ng/cli:ui",
        "report": "nx report"
