    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "prepare": "pnpm dlx simple-git-hooks",
        "pre-commit": "lint-staged",
        "format": "nx format:write all",
        "dev": "nx serve",
        "dev:web": "nx serve example-tailwind",
        "dev:mobile": "nx serve example-ionic",
        "dev:admin": "nx serve example-admin",
        "dev:server": "nx run example-server:serve",
        "db:up": "cd apps/example-server && npx drizzle-kit up",
        "db:check": "cd apps/example-server && npx drizzle-kit check",
        "db:generate": "cd apps/example-server && npx drizzle-kit generate",
        "db:migrate": "cd apps/example-server && npx drizzle-kit migrate",
        "db:local-dev": "cd apps/example-server && npx drizzle-kit push",
        "db:studio": "cd apps/example-server && npx drizzle-kit studio",
        "db:generate-erd": "drizzle-erd --in ./apps/example-server/src/core/db/schema.ts --out ./apps/example-server/erd.svg",
        "db:dbml": "tsx ./apps/example-server/dbml.ts",
        "update1of3": "nx migrate latest --verbose",
        "update2of3": "nx migrate --run-migrations --if-exists",
        "update3of3": "rm migrations.json",
        "spartan1of3": "pnpm add -D @spartan-ng/cli@latest --strict-peer-dependencies",
        "spartan2of3": "pnpm add @spartan-ng/ui-core@latest --strict-peer-dependencies",
        "spartan3of3": "pnpm dlx nx g @spartan-ng/cli:ui",
        "report": "nx report"
    },
